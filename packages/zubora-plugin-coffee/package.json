{
  "name": "zubora-plugin-coffee",
  "version": "0.0.4",
  "description": "zubora plugin for transpiling CoffeeScript.",
  "scripts": {
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json}",
    "lint": "eslint --ignore-path ../../.gitignore . --ext ts --ext js",
    "lint:fix": "npm run lint --fix",
    "clean-build": "rm -rf ./dist",
    "compile": "tsc -p ./tsconfig.build.json",
    "build": "npm run type-check && npm run format && npm run lint && npm run test && npm run clean-build && npm run compile",
    "test": "jest",
    "type-check": "tsc",
    "watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wataruoguchi/zubora.git"
  },
  "keywords": [
    "typescript",
    "coffeescript",
    "Unit Test",
    "Test",
    "zubora"
  ],
  "author": "Wataru Oguchi <wataru.oguchi.0to1@gmail.com> (https://twitter.com/watarutwt/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wataruoguchi/zubora/issues"
  },
  "homepage": "https://github.com/wataruoguchi/zubora/tree/master/packages/zubora-plugin-coffee",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "peerDependencies": {
    "coffeescript": ">= 1.8.x"
  }
}
