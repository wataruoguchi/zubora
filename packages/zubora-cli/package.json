{
  "name": "zubora-cli",
  "version": "0.0.1",
  "description": "zubora CLI",
  "bin": {
    "zubora": "bin/zubora"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json}",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "clean-build": "rm -rf ./build",
    "compile": "tsc -p .",
    "build": "yarn format && yarn lint && yarn clean-build && yarn compile",
    "test": "jest",
    "type-check": "tsc",
    "watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "build",
    "!build/__tests__",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/parser": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/traverse": "^7.6.0",
    "figlet": "^1.2.4",
    "gluegun": "^3.3.4",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@babel/types": "^7.6.1",
    "@types/babel__core": "^7.1.3",
    "@types/figlet": "^1.2.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/prettier": "^1.18.2",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "eslint": "^6.3.0",
    "husky": "^3.0.5",
    "jest": "^25.0.0",
    "lint-staged": "^9.2.5",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  }
}
