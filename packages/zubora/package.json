{
  "name": "zubora",
  "version": "0.0.4",
  "description": "The zubora module for the cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/wataruoguchi/zubora.git"
  },
  "bugs": "https://github.com/wataruoguchi/zubora/issues",
  "homepage": "https://github.com/wataruoguchi/zubora",
  "keywords": [
    "zubora",
    "Test",
    "Unit Test",
    "TypeScript",
    "AST",
    "Abstract Syntax Tree"
  ],
  "scripts": {
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json}",
    "lint": "eslint --ignore-path ../../.gitignore . --ext ts --ext js",
    "lint:fix": "yarn lint --fix",
    "clean-build": "rm -rf ./dist",
    "compile": "tsc -p ./tsconfig.build.json",
    "build": "yarn format && yarn lint && yarn clean-build && yarn compile",
    "test": "jest",
    "type-check": "tsc",
    "watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.6.0",
    "@babel/traverse": "^7.6.0"
  },
  "devDependencies": {
    "@babel/types": "^7.6.1",
    "@types/babel__core": "^7.1.3",
    "@types/jest": "^23.3.10",
    "@types/node": "^12.7.11",
    "ts-node": "^8.4.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Wataru Oguchi <wataru.oguchi.0to1@gmail.com> (https://twitter.com/watarutwt/)"
}
