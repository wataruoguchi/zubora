{
  "name": "zubora",
  "version": "0.2.1",
  "description": "The zubora module for the cli",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wataruoguchi/zubora.git"
  },
  "bugs": "https://github.com/wataruoguchi/zubora/issues",
  "homepage": "https://github.com/wataruoguchi/zubora/tree/master/packages/zubora",
  "keywords": [
    "zubora",
    "Test",
    "Unit Test",
    "TypeScript",
    "AST",
    "Abstract Syntax Tree"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "clean": "tsc -b tsconfig.build.json --clean && rm -rf ./dist",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ../../.prettierignore",
    "lint": "eslint . --ext .js,.ts --ignore-path='../../.gitignore'",
    "test": "jest --coverage",
    "clean:jest": "jest --clearCache",
    "type-check": "tsc -p tsconfig.build.json --noEmit"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/parser": "^7.6.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/traverse": "^7.6.0",
    "@babel/types": "^7.7.4",
    "@types/babel__core": "^7.1.3",
    "jest": "^26.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Wataru Oguchi <wataru.oguchi.0to1@gmail.com> (https://twitter.com/watarutwt/)"
}
