{
  "name": "zubora-page",
  "description": "The zubora page. Running Zubora in UI for zubora (\"lazy\" in Japanese) developers.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "yarn build && next export",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "start-server-and-test dev http://localhost:3000 cy:test",
    "cy:test": "cypress run --spec cypress/integration/basic.spec.js",
    "cy:init": "cypress install && cypress open"
  },
  "dependencies": {
    "@zeit/next-workers": "^1.0.0",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^2.0.7",
    "next": "9.4.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "react": "16.13.1",
    "react-ace": "^9.0.0",
    "react-dom": "16.13.1",
    "start-server-and-test": "^1.11.0",
    "styled-jsx-plugin-postcss": "^2.0.1",
    "tailwindcss": "^1.4.6",
    "worker-loader": "^2.0.0",
    "zubora": "^0.2.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@nuxt/typescript-build": "^1.0.1",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.1.0",
    "brace": "^0.11.1",
    "cypress": "^4.8.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/",
      "/.next/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "license": "MIT"
}
